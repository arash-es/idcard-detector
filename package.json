{
  "version": "0.0.0-development",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "@tensorflow/tfjs": ">=3.11.0",
    "@tensorflow/tfjs-converter": ">=3.11.0",
    "@tensorflow/tfjs-core": ">=3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@arashes/idcard-detector",
  "author": "arashray",
  "publishConfig": {
    "access": "public"
  },
  "module": "dist/idcard-detector.esm.js",
  "size-limit": [
    {
      "path": "dist/idcard-detector.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/idcard-detector.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@tensorflow/tfjs": "^3.11.0",
    "@tensorflow/tfjs-converter": "^3.11.0",
    "@tensorflow/tfjs-core": "^3.11.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^7.0.4",
    "semantic-release": "^19.0.2",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arashray/idcard-detector.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
